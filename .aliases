# Shortcuts
alias docs="cd ~/Documents/ && ls"
alias dl="cd ~/Downloads/ && ls"
alias dt="cd ~/Desktop/ && ls"
alias c++="c++ -std=c++11 -Wall"

# Directory Navigation
-='cd -'
...=../..
....=../../..
.....=../../../..
......=../../../../..
1='cd -'
2='cd -2'
3='cd -3'
4='cd -4'
5='cd -5'
6='cd -6'
7='cd -7'
8='cd -8'
9='cd -9'

# List Files
l='ls -lah'
la='ls -lAh'
ll='ls -lh'
ls='ls -G'
lsa='ls -lah'

# Enable Color in Tree
tree='tree -C'
